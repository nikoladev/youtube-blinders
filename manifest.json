{
  "name": "Youtube Blinders",
  "description": "Hide the parts of Youtube that make it hard to leave",
  "version": "1.0",
  "homepage_url": "https://github.com/nikoladev/youtube-blinders",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "permissions": [
    "*://*.youtube.com/*",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true,
    "chrome_style": true
  }
}
